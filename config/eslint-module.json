{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "plugin:isotropic/isotropic",
    "overrides": [{
        "files": [
            "./*"
        ],
        "parserOptions": {
            "ecmaFeatures": {
                "impliedStrict": false
            },
            "sourceType": "script"
        }
    }],
    "parserOptions": {
        "ecmaFeatures": {
            "globalReturn": false,
            "impliedStrict": true,
            "jsx": false
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "isotropic"
    ],
    "root": true
}
